<script>
	myDate=new Date();
	time0=myDate.getTime()+document.getElementById("time").innerHTML*1000;
	xmlhttp=new XMLHttpRequest();
	xmlhttp.onreadystatechange=state_Change;
	setInterval(function(){
		myDate2=new Date();
		if(document.getElementById("time").innerHTML>0){
			time=Math.round((time0-myDate2.getTime())/1000);
			document.getElementById("time").innerHTML=time>0?time:0;
		}else{
			xmlhttp.open("GET",'http://www.pc28yc.com/open/info.php?time='+myDate2.getTime(),true);
			xmlhttp.send();
		}
	},1000);
	function state_Change(){
		if (xmlhttp.readyState==4 && xmlhttp.status==200){
			strs=xmlhttp.responseText.split("|");
			if(document.getElementById("qihao").innerHTML!=strs[0]){
				window.location.reload(true);//整页刷新
				return;//整页刷新
				document.getElementById("qihao").innerHTML=strs[0];
				document.getElementById("sum").innerHTML=strs[1]+" + "+strs[2]+" + "+strs[3]+" =&nbsp;";
				document.getElementById("result").innerHTML=strs[4];
				document.getElementById("des").innerHTML="&nbsp;- "+strs[5]+" - "+strs[6]+" - "+strs[7];
				document.getElementById("time").innerHTML=strs[8];
				myDate=new Date();
				time0=myDate.getTime()+strs[8]*1000;
			}
		}
	}
</script>

